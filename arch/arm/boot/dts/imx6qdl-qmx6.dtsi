		pinctrl_pcie: pciegrp {
			fsl,pins = <
				MX6QDL_PAD_GPIO_2__GPIO1_IO02		0x80000000	/* PCIE_WAKE_B (Q7-156)*/
				MX6QDL_PAD_SD1_CLK__GPIO1_IO20		0x1b0b0 	/* PCIE-Reset (Q7-158)*/
			>;
		};
